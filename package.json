  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "craco build",
    "codegen": "graphql-codegen --config codegen.yml",
    "compress": "gzipper compress ./build",
    "generate-robots": "ts-node  -P ./scripts/tsconfig.json -r dotenv/config scripts/generate-robots.ts",
    "generate-sitemap": "ts-node  -P ./scripts/tsconfig.json -r dotenv/config scripts/generate-sitemap.ts",
    "test": "TZ=UTC react-scripts test",
    "test:coverage": "TZ=UTC react-scripts test --testResultsProcessor=\"./node_modules/jest-junit\" --coverage --watchAll=false",
    "browser-test": "BROWSER_TESTS_SHOW_ADMIN=true BROWSER_TESTS_SHOW_REGISTRATION=true BROWSER_TESTS_ENV_URL=https://linkedcomponents-ui.test.kuva.hel.ninja testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:local": "BROWSER_TESTS_SHOW_ADMIN=true BROWSER_TESTS_SHOW_REGISTRATION=true BROWSER_TESTS_ENV_URL=http://localhost:3000 testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:prod": "BROWSER_TESTS_LINKED_EVENTS_URL=https://api.hel.fi/linkedevents/v1 BROWSER_TESTS_ENV_URL=https://linkedevents.hel.fi testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:azure": " testcafe \"chrome:headless --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report --reporter spec,xunit:report.xml browser-tests/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report --reporter spec,custom,html:report/index.html browser-tests/",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --ext js,ts,tsx src --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "constants.ts",
      "types.ts",
      "mutation.ts",
      "query.ts",
      ".*.d.ts",
      "src/index.tsx",
      "src/common/components/imageUploader/utils.ts",
      "src/common/components/imageUploader/imageCropper/ImageCropper.tsx",
      "src/common/components/menuDropdown/menu/Menu.tsx",
      "src/common/components/table/bodyRow/BodyRow.tsx",
      "src/common/components/table/sortingHeaderCell/SortingHeaderCell.tsx",
      "src/common/components/table/tableBody/TableBody.tsx",
      "src/common/components/table/tableContainer/TableContainer.tsx",
      "src/common/components/map/",
      "src/domain/app/apollo/apolloClient.ts",
      "src/domain/app/apollo/clearCacheUtils.ts",
      "src/domain/app/apollo/utils.ts",
      "src/domain/app/hooks/usePageSettings.ts",
      "src/domain/app/i18n/i18nInit.ts",
      "src/domain/app/sentry/utils.ts",
      "src/domain/app/theme/Theme.tsx",
      "src/domain/auth/userManager.ts",
      "src/domain/auth/callbackComponent/CallbackComponent.tsx",
      "src/domain/auth/hooks/useAuth.ts",
      "src/domain/auth/hooks/useApiToken.ts",
      "src/domain/auth/oidcCallback/OidcCallback.tsx",
      "src/domain/auth/silentCallback/SilentCallback.tsx",
      "src/domain/event/formSections/timeSection/hooks/useTimeSectionContext.ts",
      "src/domain/event/formSections/timeSection/TimeSectionContext.tsx",
      "src/domain/help/pages/DocumentationPage.tsx",
      "src/domain/organizations/organizationsTable/OrganizationsTableContext.tsx",
      "src/generated/graphql.tsx",
      "src/icons/",
      "src/test/",
      "src/utils/mockAuthContextValue.ts",
      "src/utils/mockDataUtils.ts",
      "src/utils/getNocacheTime.ts",
      "src/utils/getPageHeaderHeight.ts",
      "src/utils/testUtils.ts",
      "src/utils/typescript.ts"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!react-leaflet|@react-leaflet|leaflet-draw|leaflet|react-toastify|swagger-ui-react|swagger-client|react-syntax-highlighter|react-image-crop|axios|hds-core)"
    ]
  },
  "jest-junit": {
    "outputDirectory": ".",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
