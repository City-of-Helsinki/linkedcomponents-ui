  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "compress": "gzipper compress ./build",
    "generate-robots": "esrun --tsconfig=./scripts/tsconfig.json scripts/generate-robots.ts",
    "generate-sitemap": "esrun --tsconfig=./scripts/tsconfig.json scripts/generate-sitemap.ts",
    "test": "TZ=UTC vitest",
    "test:coverage": "TZ=UTC vitest run --coverage --watch=false",
    "browser-test": "BROWSER_TESTS_LINKED_EVENTS_URL=https://linkedevents.api.dev.hel.ninja/v1 BROWSER_TESTS_ENV_URL=https://linkedevents.dev.hel.ninja testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:local": "BROWSER_TESTS_ENV_URL=http://localhost:3000 testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:prod": "BROWSER_TESTS_LINKED_EVENTS_URL=https://api.hel.fi/linkedevents/v1 BROWSER_TESTS_ENV_URL=https://linkedevents.hel.fi testcafe \"chrome --window-size='1920,1080'\" browser-tests/",
    "browser-test:azure": " testcafe \"chrome:headless --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report --reporter spec,xunit:report.xml browser-tests/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report --reporter spec,html:report/index.html browser-tests/",
    "test:e2e:install": "npx playwright install",
    "test:e2e:ci": "npx playwright test",
    "test:e2e:start": "npx playwright test",
    "test:e2e:record": "npx playwright codegen https://linkedevents.dev.hel.ninja/",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --ext js,ts,tsx src --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
